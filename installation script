# script for redhat based os
yum install git -y
sleep 5

amazon-linux-extras install java-openjdk11 -y
sleep 2

cd  /mnt

mkdir build-tools
cd build-tools
wget https://dlcdn.apache.org/maven/maven-3/3.9.1/binaries/apache-maven-3.9.1-bin.zip
unzip apache*maven*.zip

./env-var-set.sh
chmod 777 env-var-set.sh

cd ..
mkdir project
cd project
git clone url
cd game-of-life
mvn install
